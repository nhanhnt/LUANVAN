<?xml version="1.0" encoding="UTF-8"?>
<pipelines>
	<pipeline name="square">
		<parameters>
			<parameter name="input" type="int"/>
		</parameters>
		<call service="org.example.arithmatics" operation="multiply" returns="int">
	    	<parameter type="int">{input}</parameter>
	    	<parameter type="int">{input}</parameter>
		</call>		
		<transform xsl="test1.xsl"/>
	</pipeline>	
	<pipeline name="combined" serialization="xml">
		<parameters>
			<parameter name="first" type="int"/>
			<parameter name="second" type="intt"/>
		</parameters>
		<call service="org.example.arithmatics" operation="add" returns="int">
	    	<parameter type="int">{first}</parameter>
	    	<parameter type="int">{second}</parameter>
		</call>
		<call service="org.example.arithmatics" operation="multiply" returns="int">
	    	<parameter type="int">{first}</parameter>
	    	<parameter type="int">{second}</parameter>
		</call>			
		<transform method="xml" stylesheet="result.xsl"/>
	</pipeline>
</pipelines>

